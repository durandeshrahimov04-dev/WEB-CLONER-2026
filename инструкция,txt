# test_libs.py - –∑–∞–ø—É—Å—Ç–∏ –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏
import sys
libs = ['requests', 'bs4', 'lxml', 'selenium', 'playwright', 'cloudscraper', 'fake_useragent', 'pillow', 'cryptography', 'psycopg2', 'mysql.connector', 'redis', 'dnspython']
missing = [lib for lib in libs if lib not in sys.modules]
if not missing:
    print("üéâ –í–°–ï –ë–ò–ë–õ–ò–û–¢–ï–ö–ò ‚úÖ")
else:
    print("‚ùå –û—Ç—Å—É—Ç—Å—Ç–≤—É—é—Ç:", missing)




